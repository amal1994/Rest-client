{
  "name": "Rest Client",
  "description": "A client side debugger for processing http requests",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "http://*/*",
    "https://*/*",
    "https://www.google.com/*",
    "http://www.google.com/*"],
  "icons":
  {
  	"16":"icon16.png",
  	"48":"icon48.png"
  },

  "background":{
  	"scripts":["js/background.js","js/rest.js"]
  	},
  "browser_action": {
    "default_icon": "icon16.png"
 	}

}